# navigation
alias ..="cd .."
alias ...="cd ../.."
alias ....="cd ../../.."
alias .....="cd ../../../.."
alias ......="cd ../../../../.."
  # -l long format
  # -F / after dirs, * after exe, @ after symlink
  # -G colorize
  # -g suppress owner
  # -o suppress group
  # -h humanize sizes
  # -q print nongraphic chars as question marks
alias l="ls -lFGgohq"
alias la="ls -lFGgohqa" # hidden files

# git
alias gs=" git status"
alias gd=" git diff" # git diff with my custom options
alias gco="git checkout"
alias gb=" git branch"
alias ga=" git add"
alias gcm="git commit"
alias gdc="git duet-commit"
alias gp=" git push"
